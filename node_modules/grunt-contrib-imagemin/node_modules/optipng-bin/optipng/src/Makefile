all:
	cd optipng && \
	$(MAKE) && \
	cd ..

test:
	cd optipng && \
	$(MAKE) test && \
	cd ..

check: test

install:
	cd optipng && \
	$(MAKE) install && \
	cd ..

uninstall:
	cd optipng && \
	$(MAKE) uninstall && \
	cd ..

clean:
	cd optipng && \
	$(MAKE) clean && \
	cd ..

distclean:
	cd optipng && \
	$(MAKE) distclean && \
	cd ..
	-rm -f ../Makefile Makefile
